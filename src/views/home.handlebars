<div>
  {{#each products}}
              <div >
              <img src="{{this.thumbnail}}"  id="imagenes"alt="Card image cap">
              <div>
                  <h5>{{this.title}}</h5>
                  <p>{{this.description}} </p>
                  <p>Precio: {{this.price}} </p>       
                  <p>Stock: {{this.stock}} </p>   
                  <p>Code: {{this.code}} </p>                      
              </div>
          </div>
  {{/each}}
</div>